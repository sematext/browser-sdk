{
  "name": "rum-script",
  "version": "1.0.0",
  "main": "src/index.js",
  "license": "Copyright Sematext Group",
  "private": true,
  "scripts": {
    "dev": "webpack --mode development",
    "build": "webpack --mode production",
    "lint": "yarn run eslint src/ --max-warnings=0 && yarn run eslint cypress/integration/",
    "flow_stop": "yarn flow stop",
    "generate-snippet": "./node_modules/.bin/babel src/snippet.js | tail -n +2 | ./node_modules/.bin/uglifyjs -m",
    "start": "node server.js",
    "e2e_prepare": "npm run build",
    "e2e_start": "npm run start",
    "e2e_cypress": "cypress run --browser chrome --headless --spec \"cypress/integration/**/*.spec.js\"",
    "e2e_cypress_open": "cypress open",
    "e2e": "npm run e2e_prepare && start-server-and-test e2e_start http://localhost:3000 e2e_cypress",
    "e2e_open": "npm run e2e_prepare && start-server-and-test e2e_start http://localhost:3000 e2e_cypress_open"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "clean-webpack-plugin": "^0.1.19",
    "cypress": "4.12.1",
    "eslint": "^4.19.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.22.0",
    "express": "^4.16.3",
    "flow-bin": "^0.115.0",
    "html-webpack-plugin": "^3.2.0",
    "start-server-and-test": "^1.11.3",
    "terser-webpack-plugin": "^2.3.8",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.1.3"
  },
  "dependencies": {
    "core-js": "^2.5.7",
    "pako": "^1.0.11",
    "resourcetiming-compression": "^1.3.0",
    "web-vitals": "^0.2.4"
  }
}
